        Forward Send Destroy
    End_Procedure
  
    Procedure Set phmRegisterPropertyHandle Handle hProp
        Handle hSelf
        Set Private_phmRegisterPropertyHandle to hProp
        If hProp Begin
            Move Self to hSelf
            Delegate Set hProp to hSelf
        End
