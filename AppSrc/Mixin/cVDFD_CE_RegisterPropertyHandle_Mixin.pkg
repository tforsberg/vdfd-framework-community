    Procedure Destroy_Object
        //VDFD_Showln "Destroy_Object: " Self "  " (Name(Self))
        Handle hProp
        Get Private_phmRegisterPropertyHandle to hProp
        If hProp Delegate Set hProp to 0
        Forward Send Destroy_Object
    End_Procedure
  
